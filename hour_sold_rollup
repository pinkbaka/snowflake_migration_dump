select t1.ID, t1.name,sum(HOURS_SOLD_V2__C)
FROM "MTL_LOAD_PRE"."PUBLIC"."TARGET_OPPORTUNITY" t1
left join "MTL_LOAD_PRE"."PUBLIC"."TARGET_SBQQ__QUOTE__C" t2
on t1.SBQQ__PRIMARYQUOTE__C = t2.ID
left join "PC_MATILLIONLOADER_DB"."PUBLIC"."TARGET_SBQQ__QUOTELINE__C" t3
on t2.ID = t3.SBQQ__QUOTE__C
where HOURS_SOLD_V2__C is not null
group by 1,2



;


SELECT HOURS_SOLD_V2__C, * FROM "PC_MATILLIONLOADER_DB"."PUBLIC"."TARGET_SBQQ__QUOTELINE__C" 
where HOURS_SOLD_V2__C is not null;
