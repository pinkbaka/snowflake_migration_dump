SELECT t1.ID as ACCOUNT_ID, t1.Name as ACCOUNT_NAME, ZI_COMPANY_COUNTRY__C, EMPLOYEE_BAND__C, SUM(ARR__C)
FROM
(
SELECT ID, NAME, ZI_COMPANY_COUNTRY__C,NORTH_AMERICAN_COUNTRY__C,CB_COUNTRY__C, TYPE, EMPLOYEE_BAND__C, MATILLION_UPDATED_TIMESTAMP, count(*) FROM "PC_MATILLIONLOADER_DB"."PUBLIC"."TARGET_ACCOUNT"
WHERE (ZI_COMPANY_COUNTRY__C not in ('United States', 'Canada') or ZI_COMPANY_COUNTRY__C is null)
AND NAME not in ('HulaTruck', 'AppZen', 'Eightfold.ai', 'Conexiom', 'Postal.io', 'atSpoke', 'Carbon Relay', 'Servicing Solutions', 'Aqualite US', 'Siemens')
AND TYPE = 'Customer'
group by 1,2,3,4, 5, 6,7,8
order by ZI_COMPANY_COUNTRY__C desc , name asc) t1 -- NON US HQ Customers
left join
(SELECT ID, NAME, ARR__C, TYPE, CONTRACT_END_DATE__C,ACCOUNTID FROM "PC_MATILLIONLOADER_DB"."PUBLIC"."TARGET_OPPORTUNITY"
WHERE STAGENAME = 'Closed Won' 
and CONTRACT_END_DATE__C >= CURRENT_DATE()
and CONTRACT_START_DATE__C <= DATEADD(Day ,+60, current_date) 
and ARR__C ) t2
on t1.ID = t2.ACCOUNTID
group by 1,2,3,4
order by  SUM(ARR__C) desc


SELECT ID, NAME, ARR__C, TYPE, CONTRACT_END_DATE__C FROM "PC_MATILLIONLOADER_DB"."PUBLIC"."TARGET_OPPORTUNITY"
WHERE STAGENAME = 'Closed Won' 
and CONTRACT_END_DATE__C >= CURRENT_DATE()
and ARR__C 
